:root{
	--primary-color:#0d264e; 
	--secondary-color:#3562d9;
	
}
*{
	margin:0;
	padding:0;
	box-sizing:border-box;	
}
ul li{
	list-style-type:none;	
}
a{
	text-decoration:none;
}
body{
	font-family: 'Poppins', sans-serif;
}
h1,
h2,
h3,
h4,
h5,
h6{
	font-family: 'Poppins', sans-serif;
}
p{
	font-family: 'Poppins', sans-serif;
}
h1{
	font-size: 42px;
}
h2{
	font-size: 36px;
}
h3{
	font-size: 30px;
}
h4{
	font-size: 24px;
}
h5{
	font-size: 20px;
}
h6{
	font-size: 16px;
}

.container{
	max-width: 1170px;
	margin-left: auto;
	margin-right: auto;
	padding-right: 15px;
	padding-left: 15px;	
}
/*top-bar*/
.top-bar{
	background-color:#102751;	
	padding-bottom: 10px;
}
/*main header*/
.main-header{
	display:flex;
	justify-content: space-between;
	padding: 10px 0px;
}
/*top-bar-left*/

/*first list*/
.first-list{
	display:flex;
	margin:auto;
	padding: 2px 0px;
}

.address li{
	border-right: 2px solid white;	
}
.top-bar-left i{
	color:white;
	font-size: 18px;	
}
.top-bar-left ul li a{
	color:white;
	padding-right: 6px;
	font-size: 15px;
}
.top-bar-left ul li a:hover{
	text-decoration:underline;
	color:#9fb2be;
}
/*top-bar-right*/
.top-bar-right{
	padding: 2px 0px;
}
.main-navbar{
	display:flex;
}
.social-icon ul{
	display:flex;	
}
.social-icon li a{
	color: white;
    padding: 0 6px;
}
.social-icon li a:hover{
	color:#e0d9d9;
}
.first-list li i{
	color:#0997e9;
    font-size: 15px;
}
.first-list .icon{
	    padding-left: 21px;
}
.first-list .info{
	border-right:2px solid white;
	padding-right: 13px;
}
/*main logo*/
.logo{
	padding-top: 20px;
}
.logo img{
	height:65px;
}
/*menus*/
.navbar-menu{
	display: flex;
	margin: 0px;
	align-items: center;
	margin-left: auto;
	padding-right: 20px;
}
.navbar-menu li{
	list-style: none;
	padding: 12px 8px;
	font-size: 15px;
	font-weight: 600;
}
.navbar-menu a{
	text-decoration: none;
	color: #0e254e;
	padding: 13px 10px;
}
/*hover effect*/
.navbar-menu li a:hover{
	background-color:#3d547a;
	cursor:pointer;
	color:white;
}

/*button*/
.btn {
	display: inline-block;
	font-size: 15px;
	padding: 20px 23px;
	margin-top: 11px;
	margin-left: 16px;
	color: white;
	box-shadow: 9px 5px 23px 0px #c6cbe7;
    border-radius: 5px;
	background: #0a99e6; /* Old browsers */
	background: -moz-linear-gradient(left, #0a99e6 0%, #3660d9 100%); /* FF3.6-15 */
	background: -webkit-linear-gradient(left, #0a99e6 0%,#3660d9 100%); /* Chrome10-25,Safari5.1-6 */
	background: linear-gradient(to right, #0a99e6 0%,#3660d9 100%); /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
}
.btn:hover{
	background: linear-gradient(to right, #a4cce2 0%,#778fd4 100%);	
}
.row {
	display: flex;
	flex-wrap: wrap;
	margin-left: -15px;
	margin-right: -15px;
}
.col-6{
	width:50%;
	padding:15px;
	margin:0px
}
@media only screen and (max-width:1024px){
	/*menus*/
	.navbar-menu {
		padding-right: 0px;
	}
	.navbar-menu li {
		padding: 12px 8px;
		font-size: 14px;
	}
	.navbar-menu a {
		padding: 10px 5px;
	}
	
	/*btn*/
	.btn{
		font-size: 14px;
		padding: 18px 16px;
		margin-top: 16px;
		margin-left: 5px;
	}
}
/*responsive for second-header*/
@media only screen and (max-width:768px){
	.container{
		max-width:736px;
		padding-left:15px;
		padding-right:15px;
	}
	/*top bar*/
	.top-bar {
		padding-bottom: 20px;
		padding-top: 3px;
	}
	/*main hader*/
	.main-header {
		padding: 10px 0px;
	}
	/*menus*/
	.navbar-menu {
		padding-right: 0px;
		margin-left: 39px;
	}
	.navbar-menu li {
		list-style: none;
		padding: 10px 3px;
		font-size: 12px;
	}
	.navbar-menu a {
		padding: 10px 4px;
	}
	/*main-navbar button*/
	.btn{
		display: inline-block;	
		font-size: 12px;
		padding: 18px 9px;
		margin-top: 16px;
		margin-left: 8px;
	}
	/*logo*/
	.logo img {
		height: 52px;
	}
}
@media only screen and (max-width:591px){
	
/*first-navbar*/
	.main-header {
		display: inline-block;
	}
	.top-bar-left ul {
		padding: 2px 0px;
	}
	.first-list .info {
		border-right: 2px solid white;
		padding-right: 13px;
	}
	.top-bar-left ul li a {
		color: white;
		padding-right: 6px;
		font-size: 11px;
	}
	.first-list li i {
		color: #0997e9;
		font-size: 15px;
	}
	.first-list.icon {
		padding-left: 11px;
	}
	/*top bar right social-icon*/
	.social-icon li a {
		color: white;
		padding: 0 5px;
	}
	.social-icon li i{
		font-size: 15px;
	}
/*second-navbar*/	
/*for logo*/
	.logo img {
		height: 47px;
	}
	/*for menu*/
	.navbar-menu {
		display:none;
	}
	.top-bar-right{
		display: none;
	}
	.logo img {
		height: 40px;
	}
	.navicon{
		display: block;
	}
	.navicon i {
		margin-top: 11px;
		font-size: 21px;
		background-color: var(--primary-color);
		padding: 10px 8px;
		color: white;
		border-radius: 5px;
	}
	.navbar-menu a {
		padding: 12px 36px;
	}
	.main-navbar {
		display: flex;
		width: 100%;
		justify-content: space-between;
	}
	.navbar-menu{
		left: -160px;
		transition: all 0.5s;
		position: absolute;
		top: 0;
		background-color: #fff;
		height: 100vh;
		width: 146px;
		display: block;
		margin-left: 15px;
	}
	.navbar-menu.show{
		left:-17px;
	}
	.top-bar-left ul li {
		font-size: 14px;
	}
	/*for main navbar button*/
	.second-bar .button{
		display: inline-block;
	}
	.second-bar.btn{
		
		font-size: 10px;
		padding: 14px 17px;
		margin-top:11px;
		margin-left:0px;
		font-weight: 600;
	}
}
