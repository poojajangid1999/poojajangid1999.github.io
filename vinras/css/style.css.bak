:root{
	--primary-color:#6aaf07;    
}

*{
	margin:0;
	padding:0;
	box-sizing:border-box;	
}
ul li{
	list-style-type:none;	
}
a{
	text-decoration:none;
}
body{
	font-family: 'Roboto', sans-serif;
}
h1,
h2,
h3,
h4,
h5,
h6{
	font-family: 'lora', sans-serif;
}
h1{
	font-size: 42px;
}
h2{
	font-size: 36px;
}
h3{
	font-size: 30px;
}
h4{
	font-size: 24px;
}
h5{
	font-size: 20px;
}
h6{
	font-size: 16px
}
.logo img{
	width:25%;
	
}
	

